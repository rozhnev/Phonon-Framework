{
  "name": "Phonon-Framework",
  "version": "2.0.0",
  "description": "Phonon is a lightweight & scalable Javascript/CSS3/HTML5 mobile framework to develop a single page application based on Phonegap / Apache Cordova.",
  "repository": {
    "type": "git",
    "url": "https://github.com/quark-dev/Phonon-Framework.git"
  },
  "scripts": {
    "build": "node ./build/build-all.js",
    "unit": "jest ./test/unit",
    "e2e": "node ./node_modules/testcafe/bin/testcafe.js chromium ./test/e2e/**",
    "docs": "documentor docs -o docs/index.html",
    "lint": "eslint --ext .js src test/unit test/e2e"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "author": "Quarkdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quark-dev/Phonon-Framework/issues"
  },
  "homepage": "https://github.com/quark-dev/Phonon-Framework",
  "dependencies": {
    "bootstrap": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "clean-css": "^4.2.1",
    "documentor": "^0.1.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^7.0.0",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sass-lint": "^1.12.0",
    "testcafe": "^0.21.1"
  }
}
